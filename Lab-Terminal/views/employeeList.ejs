<table
  class="table table-group-divider table-responsive table-striped caption-top"
>
  <caption>
    <h3 class="text-center"><%= title %></h3>
  </caption>
  <thead>
    <tr>
      <th scope="col">Sr No</th>
      <th scope="col">Employee First Name</th>
      <th scope="col">Employee Last Name</th>
      <th scope="col">Email</th>
      <th scope="col">Department</th>
    </tr>
  </thead>
  <tbody class="table-group-divider">
    <% for (var i = 0; i < Employee.length; i++) { %>
    <tr style="background-color: #f5f5f5">
      <td scope="row"><%= i + 1 %></td>
      <td scope="row"><%= Employee[i].firstName %></td>
      <td scope="row"><%= Employee[i].lastName %></td>
      <td scope="row"><%= Employee[i].email %></td>
      <td scope="row"><%= Employee[i].department %></td>
    </tr>

    <% } %>
  </tbody>
</table>
